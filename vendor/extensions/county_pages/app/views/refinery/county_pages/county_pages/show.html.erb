<% content_for :body_content_title do %>
  <%= @county_page.name %>
<% end %>

<% content_for :body do %>
  <section>
    <h1>Name</h1>
    <p>
      <%=raw @county_page.name %>
    </p>
  </section>
  <section>
    <h1>Chapman Code</h1>
    <p>
      <%=raw @county_page.chapman_code %>
    </p>
  </section>
  <section>
    <h1>Content</h1>
    <p>
      <%=raw @county_page.content %>
    </p>
  </section>
  <section>
    <h1>Position</h1>
    <p>
      <%=raw @county_page.position %>
    </p>
  </section>
<% end %>

<% content_for :side_body do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="county_pages">
      <% @county_pages.each do |county_page| %>
        <li>
          <%= link_to county_page.name, refinery.county_pages_county_page_path(county_page) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render '/refinery/content_page' %>
