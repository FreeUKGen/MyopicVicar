<ul>
  <% if ::Refinery::CountyPages::Admin::CountyPagesController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.county_pages_admin_county_pages_path %>
    </li>
  <% end %>
  <li>
    <%= link_to t('.create_new'), refinery.new_county_pages_admin_county_page_path,
                 :class => "add_icon" %>
  </li>
<% if !searching? && ::Refinery::CountyPages::Admin::CountyPagesController.sortable? && ::Refinery::CountyPages::CountyPage.many? %>
  <li>
    <%= link_to t('.reorder', :what => "County Pages"),
                 refinery.county_pages_admin_county_pages_path,
                 :id => "reorder_action",
                 :class => "reorder_icon" %>

    <%= link_to t('.reorder_done', :what => "County Pages"),
                 refinery.county_pages_admin_county_pages_path,
                 :id => "reorder_action_done",
                 :style => "display: none;",
                 :class => "reorder_icon" %>
  </li>
<% end %>
</ul>
