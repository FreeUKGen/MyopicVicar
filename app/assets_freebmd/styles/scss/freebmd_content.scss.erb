.flex_container {
    margin-left: auto;
    display: flex;
}

/*.Navbar__Items {
  display:flex;
}*/
.Navbar__Link {
    padding-right: 35px;
    
}

/*.Navbar__Items--right {
  margin-left:auto;
}*/

.Navbar__Link-toggle {
    display: none;
}

.Navbar__Link a {
    color: #ffffff;
    float: left;
    text-align: center;
    padding: 10px 16px;
    text-decoration: none;
    font-weight: normal !important;
}

.Navbar__Link a:hover {
  background-color: #ffffff;
  color: $magenta;
}

@media only screen and (min-width: 1055px) {
    .large_screen_hide {
        display: none;
    }
}

@media only screen and (max-width: 570px) {
    .show_toggle {
        display: none !important;
    }
}

@media only screen and (max-width: 570px) {
    .Navbar__Link-toggle {
        display: none !important;
    }
}

@media only screen and (max-width: 1055px) {

    .hide_nav {
        display: none;
    }

    .show_toggle {
        display: block !important;
    }

    .flex_container,
    .Navbar,
    .copyright_banner {
        flex-direction: column;
    }

    .flex_container {
        margin-left: 0;
    }

    .flex_container {
        display: none;
    }

    .Navbar__Link-toggle {
        align-self: flex-end;
        display: initial;
        padding-right: 50px;
    }

    .brand_logo_footer {
        text-align: center;
    }

    .Navbar__Link-toggle i {
        cursor: default;
        color: #ffffff;
        padding: 7px 20px;
        text-decoration: none;
        font-size: 30px;
    }

    .Navbar__ToggleShow {
        display: flex;
        border: 5px solid $magenta;
        background-color: $magenta;
    }

    .Navbar__ToggleShow .Navbar__Link a {
        color: #ffffff;
        font-size: 0.8em;
        font-weight: bold;
        height: 50px;
    }

    .Navbar__ToggleShow .Navbar__Link {
        text-align: center;
    }
}



@media only screen and (max-width: 1300px) {
    .side_banners {
        display: none;
    }
}

/* **BMD Footer Styles** */

li i {
    padding-right: 1px;
}

.copyright_banner {
    display: flex !important;
}

.social_links a {
    display: inline-block !important;
}

.labels label {
    /*display: inline-block !important;*/
}

input:disabled {
    background: #dddddd;
}

input:disabled:not([type="submit"]):hover {
    background: #dddddd;
}

.results_block {
    text-align: center;
    color: #3E3E3E;
    border-bottom: 1px solid #707070;
}

.result_count {
    color: $magenta;
    font-style: normal;
    font-weight: normal;
    font-size: 24px;
    text-align: center;
}

.result_text {
    font-style: normal;
    font-weight: normal;
    color: #000000;
    text-align: left;
}

.bmd_search_results_table table {
    border-top: 0;
}

.search_results_table thead {
    border-bottom: 1px solid #707070;
}

.search_results_table thead tr th {
    padding: 0 !important;
}

.search_results_table tbody tr td {
    padding: 0 !important;
}

/* Vector */
.marriages {
    background-color: #99CC99;
}

.births {
    background: #CC9999;
}

.deaths {
    background: #999999;
}

.search_results_table tbody tr.births:hover {
    background: #FFCCCC !important;
}

.search_results_table tbody tr.marriages:hover {
    background: #CCFFCC !important;
}

.search_results_table tbody tr.deaths:hover {
    background: #CCCCCC !important;
}

.search_results_table tbody tr td:nth-child(1) {
    background-color: white;
}

.view_arrow {
    color: #C84182;
}

#death_age_and_dob {
    display: none;
}

#death_select_box {
    /*margin-bottom: 5px;*/
}

.bordered {
    border: 1px solid $magenta;
    background-color: #fff;
    color: #C84182;
    outline: none;
}

.border_text {
    border: 1px solid grey;
}

.bordered:hover {
    text-decoration: none;
}

.overflow-visible {
    overflow: visible !important;
}

.volunteering:hover,
.feedbck:hover {
    background-color: transparent;
    outline: 2px dashed #C84182;
    outline-offset: 7px;
}

.firstnamehelp,
.surnamehelp,
