.display_none_extra {
    @media only screen and (min-width: 1055px) {
        display: none !important;
    }
}

.nav_toggle:hover, .nav_toggle:active, .nav_toggle:focus {
    background: #ffffff;
    color: $magenta;
    outline: 5px solid #fff;
    text-decoration: none;
    cursor: pointer;
}

.capitalize {
    text-transform:capitalize !important;
}

.Navbar {
    display: flex;
    padding-left: 16px;
    background: $magenta;
    position: absolute;
    top: 150px;
    z-index: 2;
    width: 100%;
    margin-bottom: 50px;
    /**@include manual-media-query(760px) {
        max-width:  760px;
        width: 110%;
    }
    @include manual-media-query(680px) {
      max-width: 680px;
      width: 130%;
