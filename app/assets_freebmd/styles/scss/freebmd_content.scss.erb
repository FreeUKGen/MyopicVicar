.bmd_search_form {
    background: rgba(13, 195, 195, 0.05);
    padding: 20px !important;
    margin: 0px 0px 28px 0px !important;
}

.search_form li {
    margin-bottom: 25px;
}

input, select {
    padding: 10px !important;
    margin-bottom: 5px !important;
}

label {
    font-size: 1em !important;
    margin-bottom: 0px !important;
}

label.optional_attributes {
    font-weight: 200;
    vertical-align: bottom;
    margin-bottom: 5px !important;
}

input[type="checkbox"] {
    width: 17px;
    height: 17px;
    border: 1px solid gray !important;
    vertical-align: bottom;
}

*:focus {
    /*  outline: none;*/
}

select {
    display: inline-block;
    padding: 10px;
    background: none;
    border: 1px solid #ccc;
    color: black;
    width: 100%;
}

select#date_month,
select.date_year {
    /*display: inline-block;
    padding: 10px;
    background: none;
    border: 1px solid #ccc;
    color: black;
    width: 40%;*/
}

span.optional_checkbox_label {
    font-size: 0.8em;
    vertical-align: bottom;
}

.stack_fields .labeli {
    margin-bottom: -10px;
    padding-bottom: 10px;
}
.stack_fields hr {
    margin-top: 10px !important;
    margin-bottom: 10px !important
}

.header_label {
    /*border-bottom: 1px solid gray;
    margin-bottom: 2px !important;
    width: 50%;*/
    font-weight: 200 !important;
    color: #444 !important;
}

ol li label i {
    cursor: help;
    padding: 0 2px;
    color: $magenta;
    font-size: 0.8em !important;
}

input.search_form_submit_button {
    background-color: $magenta;
    color: white;
    font-weight: bold;
    border-radius: 8px;
    padding: 20px;
    width: 100px;
    border: none !important;
}

#search_form_submit_small_screen {
    background-color: #ffffff;
    color: $magenta !important;
    font-weight: bolder;
    border-radius: 8px;
    border: none !important;
}

#search_form_submit_small_screen:hover, #search_form_submit_small_screen:focus{
    background-color: #ffffff;
    color: $magenta !important;
    border-radius: 8px;
    padding:10px;
    border: none !important;
}

.grid.action {
    text-align: right;
}


.kittumani {
    @include manual-media-query(576px) {
      display: none;
    }
}

#sticky_buttons {
    @media (max-width: 576px){
        position: fixed;
        z-index: 3;
        bottom: 0;
        width: 100%;
        height: 55px;
        padding: 10px;
        left: 0;
        text-align: center;
        background: $magenta;
    }
    @media (min-width: 576px) {
      display: none !important;
    }
}

.clear_form_small_screen {
    color: #ffffff;
    text-decoration: none;
    background: none !important;
    border: none;
}

.clear_form_small_screen:focus {
    background-color: #ffffff !important;
    color: $magenta;
    border: 1px solid #ffffff;
    border-radius: 4px;
    transition: 0.1s;
    padding: 4px 4px;
}

.clear_form_small_screen:hover {
    background-color: #ffffff !important;
    color: $magenta;
    border: 1px solid #ffffff;
    border-radius: 4px;
    transition: 0.1s;
    padding: 4px 4px;
}


.clear_form {
    margin-right: 30px;
    color: $magenta;
    text-decoration: none;
    background: none !important;
    border: none;
}

.clear_form:hover, .clear_form:focus {
    background-color: #ffffff !important;
    color: #C84182;
    border: 1px solid #C84182;
    border-radius: 4px;
    transition: 0.1s;
    padding: 4px 4px;
}

select[multiple]:focus option:checked {
    background: $magenta linear-gradient(0deg, $magenta 0%, $magenta 100%);
    color: white !important;
    ;
}




