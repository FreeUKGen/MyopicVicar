.dropbtn:hover, .dropbtn:focus {
  background-color: $magenta; color: white; outline: none;
}

.dropdown {
  position: relative;
  display: inline-block;
}

.dropdown-content {
  display: none;
  overflow: auto;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  z-index: 3;
  border-radius: 0px 0 6px 6px;
  border-top: solid 5px white;
}

.dropdown-content a {
  color: black;
  padding: 7px;
  text-decoration: none;
  display: block;
  background: white;
}

.dropdown-content a:hover {background-color: #C84182; color: white; text-decoration: none;}

.show {display: block;}
.spouse_firstnames_div, .spouse_or_mother_surname_div, .age_at_death_or_date_of_birth_div, .create_postem_div {
    display: none;
}

.display_none {
    display: none;
}

#create_postem {
    color: $magenta;
    cursor: pointer;
}

#errors >li >.error {
    color: red;
}

.floating_action_buttons {
    @media (max-width: 576px) {
      border: none;
      width: 55px;
      height: 55px;
      border-radius: 50%;
      background-color: $magenta;
      position: fixed;
      bottom: 75px;
      right: 30px;
      cursor: pointer;
      color: white;
      box-shadow: 2px 2px 2px 2px $project_color_faded;
    }
}

.small_screen_actions {
    @media (min-width: 576px) {
      display: none !important;
    }

}

.floating_action_buttons_left {
    @media (min-width: 576px) {
      display: none !important;
    }
    @media (max-width: 576px) {
      border: none;
      width: 35px;
      height: 35px;
      border-radius: 50%;
      background-color: white;
      position: fixed;
      top: 500px;
      right: 40px;
      cursor: pointer;
      color: $magenta;
      box-shadow: 1px 1px 1px 1px grey;
    }
}

.floating_action_buttons:active{
   box-shadow: 2.1px 2.1px 2.1px 2.1px $project_color_faded;
   transform: scale(1.05);
   transition-duration: 0.3s;
 }

.update_results {
    padding: 10px !important;
    width: auto !important;
}

.update_results:hover {
    color: blue;
}

.postems:focus .fa-envelope:before, .postems:hover .fa-envelope:before {
    content: "\f658";
    font-size: 1.2em !important;
}
.table_link:focus, .table_link:hover {
    outline: 2px dashed $magenta;
    outline-offset: -5px;
    text-decoration: none;
    background-color: "#fff"
}
.table_link:focus i, .table_link:hover i {
    display: none;
}
.postems_link:focus:before, .postems_link:hover:before {
    content: "Postems"
}

.scans_link:focus:before, .scans_link:hover:before {
    content: "Scans"
}

.save_link:focus:before, .save_link:hover:before {
    content: "Save Entry"
}

.saved_link:focus:before, .saved_link:hover:before {
    content: "Entry is saved"
}

.scan_files:focus, .scan_files:hover .fa-camera:before {
    content: "\f1c5";
    font-size: 1.2em !important;
    transition-duration: 0.3s;
    outline: 2px dashed $magenta;
    outline-offset: 7px;
    text-decoration: none;
}


.postems_d .postem_text,
.scan_d .scan_text,
.idk_d .idk_text {
  display: none;


  /* Position the tooltip */
 
}

.postems_d:focus .postem_text, .postems_d:hover .postem_text,
.scan_d:focus .scan_text, .scan_d:hover .scan_text,
.idk_d:focus .idk_text, .idk_d:hover .idk_text,
{
  display: block;
}

.answer_explanation {
    border: 1px solid;
    padding: 10px;
}

.report_form, .read_answer {
    display: none;
}


.right_ttip {
    top: -5px !important;
    left: 25% !important;
}


