<% breadcrumb :freecen2_districts_chapman,  @chapman_code, @year%>
<h2 style="text-align: center">FreeCen2 Districts for <%=ChapmanCode.name_from_code(@chapman_code)%> (<%=@chapman_code%>) in <%= @year%></h2>
<% if %w[data_manager system_administrator].include?(@user.person_role)%>
  <div style="text-align: center">
    <%= link_to 'Add Distict', new_freecen2_district_path(year: @year, chapman_code: @chapman_code, type: @type), data: { confirm: 'Are you sure?' }, :class => "btn   btn--small", title: 'Adding a District is restricted to data managers and system administrators'%>
  <% end%>
  <%= render 'basic_index' %>