<% breadcrumb :freecen2_districts,  @chapman_code, @year%>
<h2 style="text-align: center">FreeCen2 Districts for <%=@chapman_code%> <%=@year%></h2>
<%= render 'flash_notice' %>
<table style="margin-left:25%" class="table--bordered t50 table--data table--striped " >
  <thead >
    <tr >
      <th >District</th>
      <th>1841</th>
      <th>1851</th>
      <th>1861</th>
      <th>1871</th>
      <th>1881</th>
      <th>1891</th>
      <th>1901</th>
      <th>1911</th>
    </tr>
  </thead>
  <tbody >
    <% @freecen2_districts_distinct.each do |freecen2_district| %>
      <tr >
        <td> <%= freecen2_district %></td>
        <% @census.each do |year| %>
          <td> <%= district_year(freecen2_district, year)%></td>
        <% end%>
        <td></td>
      </tr>
    <% end%>
  </tbody >
</table>