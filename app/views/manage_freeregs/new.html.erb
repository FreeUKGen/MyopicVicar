<p style="text-align: center"><b>From here you can manage FreeREG</b></p>
  
   

<%= semantic_form_for(@manage_freeregs) do |f| %>
<div style="float:left;"> <%= f.input :userid, { :as => :select, :include_blank => false, :label => ".",:prompt => "Manage Userids",  :required => false,:size => 5 , :collection => @options_userids,:input_html => {'onchange' => @location_manage_userid} }%> </div>
<div style="float:left;"> <%= f.input :userid, { :as => :select, :include_blank => false,  :label => ".", :prompt => "Manage SCs",
  :required => false, :collection => @options_manage_sc,:width => 10 ,:input_html => {'onchange' => @location_manage_sc} }%></div>
<div style="float:left;"> <%= f.input :userid, { :as => :select, :include_blank => false,  :label => ".", :prompt => "Manage CCs",
  :required => false, :collection => @options_manage_cc,:width => 10 ,:input_html => {'onchange' => @location_manage_cc} }%></div>
<% end %> 
<div style="float:left;"><%= button_to 'Manage Country Coordinators', countries_path, method: :get  %></div>
<div style="float:left;"><%= button_to "Manage County Data ", manage_counties_path, method: :get  %></div>
<div style="float:left;"><%= button_to 'Retrieve Attic files', my_saved_searches_path, method: :get %></div>
<div style="float:left;"><%= button_to "Retrieve Activity logs ", my_saved_searches_path, method: :get   %> </div>
<div  style="float:left;"> <%= button_to 'Review Search Performance', search_query_report_path, method: :get %> </div>
<div  style="float:left;"> <%= button_to 'Review Problem Feedback', feedbacks_path, method: :get %> </div>


