<% case controller.controller_name %>
<% when 'assignments' %>
		<% if params[:assignment_list_type] == 'all' || (!params[:assignment].nil? && params[:assignment][:assignment_list_type] == 'all') %>
  		<div style="clear:both;"></div><br>
  		<p style="text-align: center"><b>Assignments</b> of Syndicate <b><%= session[:syndicate] %></b>
		<% else %>
  		<div style="clear:both;"></div><br>
  		<p style="text-align: center"><b>Images Assignment</b> of Image Group <b><%= @group.group_name %></b> of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>
  	<% end %>
<% when 'sources' %>
		<% if controller.action_name == 'index' %>
			<div style="clear:both;"></div><br>
			<p style="text-align: center"><b>Sources</b> for <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>
		<% else %>
			<div style="clear:both;"></div><br>
			<p style="text-align: center"><b><%= @source.source_name %></b> source for <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>
		<% end %>
<% when 'image_server_groups' %>
		<% if controller.action_name == 'index' %>
			<div style="clear:both;"></div><br>
			<p style="text-align: center"><b>Image Groups</b> for the <b><%= @source.source_name %></b> Source of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>
		<% else %>
			<div style="clear:both;"></div><br>
			<p style="text-align: center"><b>Image Group</b> for the <b><%= @source.source_name %></b> Source of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>
		<% end %>
<% when 'image_server_images' %>
		<% if controller.action_name == 'index' %>
			<div style="clear:both;"></div><br>
			<p style="text-align: center"><b>Images</b> of Image Group <b><%= @group.group_name %></b> of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>
		<% else %>
			<div style="clear:both;"></div><br>
			<p style="text-align: center"><b>Image</b> of Image Group <b><%= @group.group_name %></b> of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>
		<% end %>
<% end %>
