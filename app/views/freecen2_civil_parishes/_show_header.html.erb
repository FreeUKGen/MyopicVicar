<% breadcrumb :show_freecen2_civil_parish,  @freecen2_civil_parish, @piece.chapman_code, @piece.year %>
<h2 style="text-align: center">Details of FreeCen2 Civil Parish for Piece <%=@piece.number%> in <%=@piece.chapman_code%> for <%=@piece.year%></h2>
<%= render 'flash_notice' %>
<% if %w[data_manager system_administrator].include?(@user.person_role)%>
  <div style="text-align: center">
    <%= link_to 'Edit Civil Parish', edit_freecen2_civil_parish_path(@freecen2_civil_parish) , :class => "btn   btn--small"%>
  </div>
<% end%>