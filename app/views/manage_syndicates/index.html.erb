Page Identification Manage Syndicates:index<p>
<%if session[:muliple] == false%>
<p><%= @first_name%> you are managing the <b><%= @syndicates %></b> syndicate </p>
 <% end %>

 

  <%= semantic_form_for @manage_syndicate do |f| %>

  <%if  session[:muliple] == true%>
      <%= f.input :syndicate, 
        { :as => :select, :label => "#{@first_name} you have multiple syndicates. Which would you like to work with?",
          :include_blank => false, :required => true, :collection => @syndicates, :size => 15 }  %>
    <% end %>

  <br>
  <div  style="float:left;"> <%= f.input :action, { :as => :select, :include_blank => false, :collection => ['Review Members listed alphabetically', 'Upload New Batch','Review Batches with errors','Review Batches listed by filename', 'Review Batches with errors','Review Batches listed by userid then filename','Review Batches listed by uploaded date',  ], :label => "What would you like to do? ",  :size => 10  }%></div>


    <div  style="float:left;"><%= f.action :submit, :as => :input, :label => 'Select'   %></div>
   <% end %>

<div style="clear:both;"> </div>

  <br>        
 
<div  style="float:left;"> <%= button_to 'Resource Management', manage_resource_path(@user), method: :get %> </div>
