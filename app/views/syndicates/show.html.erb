Page Identification Syndicates:show<p>
<p style="text-align: center"><b>Details for a Syndicate </p></b>

<div style="float:left;"> <%= button_to 'Edit details', edit_syndicate_path(@syndicate), method: :get  %> </div>

<div  style="float:left;"> <%= button_to 'FreeREG management', manage_freeregs_path, method: :get %> </div>
<div style="float:left;">  <%= button_to_function "Previous Page", "history.back()" %> </div>
<div style="clear:both;"> </div><br>

  <% if flash[:notice] %>
    <div id="notice" style="color:blue"><br>
    <%= flash[:notice] %> <br>
    </div>
   <% end %>


<div id="main"style="color:black">
<table  CLASS="boldtable" style="width: 100%">
  <tr>
    
    <th style="width:25%">Field</th>
    <th style="width:50">Value</th>
     
  </tr>

  <tr> 
   <td>Syndicate</td><td > <%= @syndicate.syndicate_code %>  </td>
  </tr><tr>
   <td>Syndicate Coordinator Userid</td><td > <%= @syndicate.syndicate_coordinator %>  </td>   
   </tr>
   <tr>
   <td>Syndicate Coordinator Name</td><td > <%= @person unless @person.nil? %> </td>   
   </tr>
   <tr>
   <td>Previous Syndicate Coordinator Userid</td><td > <%= @syndicate.previous_syndicate_coordinator %>  </td>   
   </tr>
   <tr>
   <td>Previous Syndicate Coordinator Name</td><td > <%= @previous_person unless @previous_person.nil? %>  </td>   
   </tr>
   <tr>
   <td>Syndicate Description</td><td > <%= @syndicate.syndicate_description %>  </td>   
   </tr>
  <tr>
   <td>Syndicate Notes</td><td > <%= @syndicate.syndicate_notes %>  </td>   
   </tr>
 <tr>
   <td>Syndicate Last Updated</td><td > <%= @syndicate.u_at.strftime("%d %b %Y") %>  </td>   
   </tr>

</table>
</div>

<div style="float:left;"> <%= button_to 'Edit details', edit_syndicate_path(@syndicate), method: :get  %> </div>

<div  style="float:left;"> <%= button_to 'FreeREG management', manage_freeregs_path, method: :get %> </div>
<div style="float:left;">  <%= button_to_function "Previous Page", "history.back()" %> </div>
<div style="clear:both;"> </div><br>