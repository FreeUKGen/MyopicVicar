<%= render 'show_header' %>
<div class="grid">
  <section class="island ">
    <div class="grid__item ">
      <table   class="table--bordered table--striped  table--data">
        <colgroup >
          <col class=t40>
          <col class=t60>
        </colgroup>
        <tr>
          <th class="caps">Field</th>
          <th  class="caps">Value</th>
        </tr>
        <tr>
          <td>Census type:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.census_type %> </td>
        </tr>
        <tr>
          <td>Raw year:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.raw_year %> </td>
        </tr>
        <tr>
          <td>Full year:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.full_year  %></td>
        </tr>
        <tr>
          <td>Piece:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.piece  %></td>
        </tr>
        <tr>
          <td>Series:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.series  %></td>
        </tr>
        <tr>
          <td>Parish:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.sctpar  %></td>
        </tr>
        <tr>
          <td>Digest:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.file_digest  %></td>
        </tr>
        <tr>
          <td>Errors:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.file_errors  %></td>
        </tr>
        <tr>
          <td>Transcriber name:</td>
          <td class="weight--semibold">
            <%= @freecen1_vld_file.transcriber_name  %></td>
        </tr>
      </p>
    </table>
  </div>
</section>
</div>
