<% breadcrumb :freecen1_vld_file,  @chapman_code, @freecen1_vld_file %>
<h1 style="text-align: center">Listing of <%= @freecen1_vld_file.file_name %> for <%= @chapman_code%></h1>
<%= render 'flash_notice' %>
<div style="text-align: center">
  <% if %w[data_manager county_coordinator master_county_coordinator system_administrator].include?(@user.person_role)%>
    <%= edit_freecen1_vld_file %>
    <%= link_to 'Download file as CSV ', csv_download_freecen1_vld_file_path(file: @freecen1_vld_file.id) , :class => "btn btn--small" %>
    <%= link_to 'Delete Vld File', freecen1_vld_file_path(@freecen1_vld_file), method: :delete, data: { confirm: 'Are you sure you want to delete this Vld File?' } , :class => "btn  btn--small"%>
  <% end%>
  <%= link_to 'Show entries ', freecen1_vld_entries_path(file: @freecen1_vld_file.id) , :class => "btn btn--small" %>
</div>