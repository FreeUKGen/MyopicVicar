  <%= f.input :groom_father_title,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_father_occupation,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_mother_forename,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_mother_surname,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_mother_title,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_mother_occupation,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_forename,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_surname ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :bride_title ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :bride_age ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :bride_condition,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_abode,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
