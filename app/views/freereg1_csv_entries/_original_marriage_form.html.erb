<%= simple_form_for @freereg1_csv_entry, :html => { :class => "grid"}  do |f| %>
  <%= f.input :register_entry_number,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :marriage_date,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_forename,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_surname ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :groom_age ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :groom_parish,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_condition ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :groom_occupation,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_abode ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :bride_forename,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_surname ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :bride_age ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :bride_parish,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_condition,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_occupation,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_abode,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_father_forename,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_father_surname,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :groom_father_occupation,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_father_forename ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :bride_father_surname,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :bride_father_occupation,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.simple_fields_for :multiple_witnesses do |multiple| %>
    <%= multiple.input :witness_forename,:input_html => {:class => " simple_form_bgcolour  ", :size => 30 } %>
    <%= multiple.input :witness_surname,:input_html => { :class => " simple_form_bgcolour  ",:size => 30 } %>
    <%= multiple.input :_destroy, :as => :boolean ,:input_html => {:class => " simple_form_bgcolour  "} %>
  <% end %>
  <%= f.input :notes, :label => 'Notes in register', :as => :text, :input_html => {:class => " simple_form_bgcolour  ",   :placeholder => "Enter your notes",  :size => "29x5" }%>
  <%= f.input :film,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :film_number ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :record_type , :input_html => { :disabled => true  }  %>
  <%= f.input :line_id, :input_html => { :disabled => true  }%>
  <%= f.input :file_line_number, :input_html => { :disabled => true  } %>
  <%= f.hidden_field :register_type, :value=>@register_type %>
  <%= f.hidden_field :county, :value=>@chapman_code %>
  <%= f.hidden_field :place, :value=>@place_name %>
  <%= f.hidden_field :church_name, :value=>@church_name %>
  <%= f.button :submit, 'Submit'   %>
<% end %>