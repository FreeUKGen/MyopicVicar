<%= simple_form_for @freereg1_csv_entry, :html => { :class => "grid"}  do |f| %>
  <%= f.input :register_entry_number,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }  %>
  <%= f.input :birth_date ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :baptism_date ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :confirmation_date ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :received_into_church_date ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_forename ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_surname ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_title ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_age ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_relationship ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_sex, collection: ['M','F','?'],:input_html =>{:class => " simple_form_bgcolour simple_form_position overide_selection_field_width", :size => 3}%>
  <%= f.input :person_abode ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_condition ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_status ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_occupation ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_place_birth ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :person_county_birth ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :father_forename ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :father_surname,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :father_title,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :father_abode,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :father_occupation,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :father_place,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :father_county,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 } %>
  <%= f.input :mother_forename ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :mother_surname ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :mother_title ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :mother_abode ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :mother_occupation ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :mother_condition_prior_to_marriage ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :mother_place_prior_to_marriage ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :mother_county_prior_to_marriage ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :private_baptism ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.simple_fields_for :multiple_witnesses do |multiple| %>
    <%= multiple.input :witness_forename,:input_html => { :size => 30 } %>
    <%= multiple.input :witness_surname,:input_html => { :size => 30 } %>
    <%= multiple.input :_destroy, :as => :boolean %>
  <% end %>
  <%= f.input :notes, :label => 'Notes in register', :as => :text, :input_html => {:class => " simple_form_bgcolour  ",   :placeholder => "Enter notes from register",  :size => "29x5" }%>
  <%= f.input :notes_from_transcriber, :label => 'Transcriber Notes', :as => :text, :input_html => {:class => " simple_form_bgcolour  ",   :placeholder => "Enter your notes from and about entry",  :size => "29x5" }%>
  <%= f.input :film   ,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }   %>
  <%= f.input :film_number,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 30 }%>
  <%= f.input :image_file_name,  :input_html => {:class => " simple_form_bgcolour  ",  :size => 50 }%>
  <%= f.input :record_type, :input_html => {:disabled => true  ,  :size => 30}%>
  <%= f.input :line_id, :input_html => {:disabled => true  ,  :size => 30}%>
  <%= f.input :file_line_number, :input_html => { :disabled => true,  :size => 30} %>
  <%= f.hidden_field :register_type, :value=>@register_type %>
  <%= f.hidden_field :county, :value=>@chapman_code %>
  <%= f.hidden_field :place, :value=>@place_name %>
  <%= f.hidden_field :church_name, :value=>@church_name %>
  <%= f.button :submit, 'Submit'   %>
<% end %>