<footer class="site__footer">
  <div class="island  island--magenta  flush--bottom">
    <div class="container  text--center">
      <% case appname.downcase%>
      <% when 'freereg' %>
      <%= render 'layouts/freereg_caution' %>
      <% when 'freecen' %>
      <%= render 'layouts/freecen_caution' %>
      <% when 'freebmd' %>
      <%= render 'layouts/freebmd_caution' %>
    <% end %>
  </div>
</div>
<div class="icon__honeycomb--bottom-left  island  flush--bottom">
  <div class="container  text--center">
    <%= render "layouts/other_projects" %>
  </div>
</div>
<%= render 'layouts/basic_footer' %>
</footer>