<% if session[:manage_user_origin] == 'manage syndicate' %>
	<% breadcrumb :syndicate_image_server_group, @county, @place, @church, @register, @source, @group %>
<% else %>
	<% breadcrumb :image_server_group, @county, @place, @church, @register, @source, @group %>
<% end %>

<div style="clear:both;"></div><br>
<p style="text-align: center">Image Groups for the <b><%= @source.source_name %></b> Source of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>

<% if flash[:notice] %>
  <div id="notice" style="color:blue">
    <%= flash[:notice] %><% flash.clear %>
  </div><br>
<% end %>

<div class="grid">
	<%= render 'form' %>
</div>