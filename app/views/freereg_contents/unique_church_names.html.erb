<% title "#{@church_name} #{@place_name} #{@county} names" %>
<div class="container">
  <section class="island ">
    <div style="text-align: center">
      <% if @referer.present?%>
        <%= link_to 'Back to Search Record', @referer, :class => "btn   btn--small"%>
      <% else%>
        <%= link_to 'County Selection', new_freereg_content_path, method: :get, :class => "btn   btn--small"%>
        <%= link_to 'Another Place in this County', freereg_contents_path, method: :get, :class => "btn   btn--small"%>
        <%= link_to 'This Place', show_place_freereg_content_path(@place.id), :class => "btn   btn--small" %>
        <%= link_to 'This Church', show_church_freereg_content_path( @church.id), :class => "btn   btn--small" %>
      <% end%>
    </div>
    <%= render 'flash_notice' %>
    <br>
    <h3 class=" text--center">Unique names for people in  <%=@church.church_name %> of <%= @place_name %> in <%= @county %> </h3>
    <%= render 'unique_names' %>
  </section>
</div>