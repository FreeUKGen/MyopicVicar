<%= semantic_form_for @freereg_contents, :html => { :class => "grid"} do |f| %>
  
    <%= f.input :county, 
        { :as => :select, :include_blank => false, :required => true, :collection => @options,
        :size => 2 , :class => "text-input",  :label => "Type of Register :",
        :input_html => { :class => "select" }, 
        :wrapper_html => { :class => "grid__item  one-whole  palm-one-whole" }
        }%>

    <%= f.action :submit, :as => :input, :label => 'Select'  %>
  
<% end %>
              