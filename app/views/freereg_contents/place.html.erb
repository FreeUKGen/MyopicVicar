<% title "#{@place.place_name} #{@county} transcriptions " %>
<div class="container">
  <section class="island ">
    <div class="text--center">
      <%= link_to 'Back to Search Record', :back, :class => "btn   btn--small"%>
      <%= link_to 'Names', unique_place_names_path(@place.id,ref: @referer), method: :get, :class => "btn   btn--small"%>
    </div>
    <br>
    <h2 class=" text--center"><%= @place.place_name %> in <%= @place.county %> </h2>
    <div class="grid">
      <div class="grid__item desk-one-half lap-one-half palm-one-whole">
        <div class="grid__item one-whole">
          <%= render 'place' %>
        </div>
        <div class="grid__item one-whole">
          <div class = "adsense_box push--ends">
            <span>Advertisement</span>
            <%= my_heritage_300_250_second %>
          </div>
        </div>
      </div>
      <div class="grid__item desk-one-half lap-one-half palm-one-whole">
        <%= render 'decade' %>
      </div>
    </div>
  </section>
</div>
