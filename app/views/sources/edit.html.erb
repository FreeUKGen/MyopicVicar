<% case @source.source_name %>
  <% when 'Image Server' %>
    <% case session[:ig_allocation] %>
      <% when 'by_syndicate' %>
        <% breadcrumb :sort_by_syndicate_image_source, @county, @place, @church, @register, @source %>
      <% when 'by_place' %>
        <% breadcrumb :sort_by_place_image_source, @county, @place, @church, @register, @source %>
      <% when 'by_all' %>
        <% breadcrumb :sort_by_all_image_source, @county, @place, @church, @register, @source %>
      <% when 'by_unallocate' %>
        <% breadcrumb :sort_by_unallocate_image_source, @county, @place, @church, @register, @source %>
      <% else %>
        <% breadcrumb :county_image_source, @county, @place, @church, @register, @source %>
    <% end %>
  <% when 'Other Server1' %>
    <% breadcrumb :county_other_server1, @county, @place, @church, @register, @source %>
  <% when 'Other Server2' %>
    <% breadcrumb :county_other_server2, @county, @place, @church, @register, @source %>
  <% when 'Other Server3' %>
    <% breadcrumb :county_other_server3, @county, @place, @church, @register, @source %>
<% end %>

<% title "Source - edit" %>
<div style="clear:both;"></div><br>
<p style="text-align: center"><b><%= @source.source_name %></b> source for <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>

<% if flash[:notice] %>
  <div id="notice" style="color:blue">
   	<%= flash[:notice] %><% flash.clear %>
  </div><br>
<% end %>

<%= render 'edit' %>

