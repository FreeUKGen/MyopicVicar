<% breadcrumb :freecen2_site_statistics,  @county%>
<h2 style="text-align: center"><%= @county%> Freecen2 Site Statistics on <%= @inverval_end.strftime('%d/%b/%Y') %></h2>
<h4 style="text-align: center">The New columns list the changes in the previous 30 days</h4>
<div class='scrollable'>
  <table  class="table--bordered table--data table--striped " >
    <thead >
      <tr>
        <th class='sticky-header'>Year/Period End</th>
        <th class='sticky-header'>Search Records</th>
        <th class='sticky-header'>On Line Vld Files</th>
        <th class='sticky-header'>Vld Files</th>
        <th class='sticky-header'>Vld Records</th>
        <th class='sticky-header'>Csv Files</th>
        <th class='sticky-header'>Csv Files Incor</th>
        <th class='sticky-header'>Csv Records</th>
        <th class='sticky-header'>Csv Records Incor</th>
        <th class='sticky-header'>Individuals</th>
        <th class='sticky-header'>Dwellings</th>
        <th class='sticky-header'>New Vld Files</th>
        <th class='sticky-header'>New Vld Records</th>
        <th class='sticky-header'>New Csv Files</th>
        <th class='sticky-header'>New Csv Records</th>
        <th class='sticky-header'>New Csv Records Incor</th>
      </tr>
    </thead>
    <tbody>
      <% Freecen::CENSUS_YEARS_ARRAY.each do |census|%>
        <tr>
          <td><%= census %></td>
          <td><%= @county_stats[census][:search_records] %></td>
          <td><%= @county_stats[census][:vld_files_on_line] %></td>
          <td><%= @county_stats[census][:vld_files] %></td>
          <td><%= @county_stats[census][:vld_entries] %></td>
          <td><%= @county_stats[census][:csv_files] %></td>
          <td><%= @county_stats[census][:csv_files_incorporated] %></td>
          <td><%= @county_stats[census][:csv_entries] %></td>
          <td><%= @county_stats[census][:csv_entries_incorporated] %></td>
          <td><%= @county_stats[census][:individuals] %></td>
          <td><%= @county_stats[census][:dwellings] %></td>
          <td><%= @county_stats[census][:added_vld_files] %></td>
          <td><%= @county_stats[census][:added_vld_entries] %></td>
          <td><%= @county_stats[census][:added_csv_files] %></td>
          <td><%= @county_stats[census][:added_csv_entries] %></td>
          <td><%= @county_stats[census][:added_csv_entries_incorporated] %></td>
        </tr>
      <% end %>
      <tr>
        <td><%= @inverval_end.strftime('%d/%b/%Y') %></td>
        <td><%= @county_stats[:total][:search_records] %></td>
        <td><%= @county_stats[:total][:vld_files_on_line] %></td>
        <td><%= @county_stats[:total][:vld_files] %></td>
        <td><%= @county_stats[:total][:vld_entries] %></td>
        <td><%= @county_stats[:total][:csv_files] %></td>
        <td><%= @county_stats[:total][:csv_files_incorporated] %></td>
        <td><%= @county_stats[:total][:csv_entries] %></td>
        <td><%= @county_stats[:total][:csv_entries_incorporated] %></td>
        <td><%= @county_stats[:total][:individuals] %></td>
        <td><%= @county_stats[:total][:dwellings] %></td>
        <td><%= @county_stats[:total][:added_vld_files] %></td>
        <td><%= @county_stats[:total][:added_vld_entries] %></td>
        <td><%= @county_stats[:total][:added_csv_files] %></td>
        <td><%= @county_stats[:total][:added_csv_entries] %></td>
        <td><%= @county_stats[:total][:added_csv_entries_incorporated] %></td>
      </tr>
    </tbody>
  </table>
</div>
<br />