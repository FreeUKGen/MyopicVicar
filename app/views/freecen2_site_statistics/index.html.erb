<% breadcrumb :freecen2_site_statistics,  @county%>
<h2 style="text-align: center">Listing <%= @county%> Freecen2 Site Statistics</h2>
<h4 style="text-align: center">The New columns list the changes in the previous 30 days</h4>
<div class='scrollable'>
  <table  class="table--bordered table--data table--striped " >
    <thead >
      <tr>
        <th class='sticky-header'>Interval end</th>
        <th class='sticky-header'>Searches</th>
        <th class='sticky-header'>Search Records</th>
        <th class='sticky-header'>On Line Vld Files</th>
        <th class='sticky-header'>Vld Files</th>
        <th class='sticky-header'>Vld Records</th>
        <th class='sticky-header'>Csv Files</th>
        <th class='sticky-header'>Csv Files Incor</th>
        <th class='sticky-header'>Csv Records</th>
        <th class='sticky-header'>Csv Records Incor</th>
        <th class='sticky-header'>Individuals</th>
        <th class='sticky-header'>Dwellings</th>
        <th class='sticky-header'>New Vld Files</th>
        <th class='sticky-header'>New Vld Records</th>
        <th class='sticky-header'>New Csv Files</th>
        <th class='sticky-header'>New Csv Records</th>
        <th class='sticky-header'>New Csv Records Incor</th>
      </tr>
    </thead>
    <tbody>
      <% @freecen2_site_statistics.each do |statistic| %>
        <tr>
          <td><%= statistic.interval_end.strftime('%d/%b/%Y') %></td>
          <td><%= statistic.searches %></td>
          <td><%= statistic.records[:total][:total][:search_records] %></td>
          <td><%= statistic.records[:total][:total][:vld_files_on_line] %></td>
          <td><%= statistic.records[:total][:total][:vld_files] %></td>
          <td><%= statistic.records[:total][:total][:vld_entries] %></td>
          <td><%= statistic.records[:total][:total][:csv_files] %></td>
          <td><%= statistic.records[:total][:total][:csv_files_incorporated] %></td>
          <td><%= statistic.records[:total][:total][:csv_entries] %></td>
          <td><%= statistic.records[:total][:total][:csv_entries_incorporated] %></td>
          <td><%= statistic.records[:total][:total][:individuals] %></td>
          <td><%= statistic.records[:total][:total][:dwellings] %></td>
          <td><%= statistic.records[:total][:total][:added_vld_files] %></td>
          <td><%= statistic.records[:total][:total][:added_vld_entries] %></td>
          <td><%= statistic.records[:total][:total][:added_csv_files] %></td>
          <td><%= statistic.records[:total][:total][:added_csv_entries] %></td>
          <td><%= statistic.records[:total][:total][:added_csv_entries_incorporated] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<br />