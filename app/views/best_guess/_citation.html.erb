<% citation_types = ['wikitree_ref', 'wikitree_inline', 'familytree', 'family_tree_maker', 'legacy_family_tree', 'mla', 'chicago', 'evidence_explained', 'wikipedia'] %>
<% citation_types.each do |cite| %>
	<%= render partial: "#{cite}_freebmd_citation", locals: {entry: @current_record} if lookup_context.find_all("best_guess/_#{cite}_freebmd_citation").any? %>
<% end %>