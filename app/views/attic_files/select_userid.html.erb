<% breadcrumb :select_attic_files%>
<h2 class=" text--center">Select the userid for whom you wish to review attic files</h2>

<% if flash[:notice] %>
<div id="notice" style="color:red"><br>
  <%= flash[:notice] %>  <% flash[:notice] = nil%> <br>
</div>
<% end %>
<%= semantic_form_for @attic_file do |f| %>
  <%= f.input :userid, { :as => :select, :label => "Please select a Userid from the list", :required => false, :collection => @options,:width => 10 ,:input_html => {:class => " simple_form_bgcolour simple_form_position overide_selection_field_width", :size => 10 , 'onchange' => @location} }%>
<% end %>
