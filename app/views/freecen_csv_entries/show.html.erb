<% breadcrumb :show_record,  @freecen_csv_entry,  @freecen_csv_file %>
<% breadcrumb :zero_year_record_detail, @freecen_csv_entry, @freecen_csv_file if params[:zero_record]== "true" && session[:from].blank? %>
<% breadcrumb :show_zero_year_entry, @freecen_csv_entry, @freecen_csv_file if params[:zero_listing] == "true" && session[:from].blank? %>
<h3 style="text-align: center">Showing record <b><%= @freecen_csv_entry.file_line_number %></b> for batch: <b><%= @freecen_csv_file_name %></b> (<%= @file_owner %>) in <b><%= @register_name %></b> of <b> <%= @church_name%></b> in <b><%= @place_name %></b> of <b><%= @county %></b><br>
</h3>
<%if session[:from].present? %>
  <h4 style="text-align: center"> From UCF lookup</h4>
<% end%>
<% unless session[:userid].nil?%>
  <div  style="text-align: center">
    <%= link_to 'Edit Record',  edit_freecen_csv_entry_path(@freecen_csv_entry, "zero_record" => @get_zero_year_records, "zero_listing" => @zero_year), method: :get, :class => "btn btn--small" %>
    <%= link_to 'Destroy', freecen_csv_entry_path(@freecen_csv_entry), data: { confirm:  'Are you sure?'}, method: :delete , :class => "btn btn--small"  %>
    <%= link_to 'Edit Embargo',  edit_embargo_freecen_csv_entry_path(@freecen_csv_entry, "zero_record" => @get_zero_year_records, "zero_listing" => @zero_year), method: :get, :class => "btn btn--small" if @embargo_permitted && session[:from].blank? %>
  </div>
  <div style="clear:both;color:'black'"> </div>
  <br>
<% end %>
<%= render 'flash_notice' %>
<section id="search-detail" >
  <%= render 'entry_detail' %>
</section>
<br>