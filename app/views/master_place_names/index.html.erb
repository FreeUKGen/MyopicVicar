Page Identification Master Place:index<p>


<p>All Places for <b><%= @county  %></b>. Select the Genuki link to see the specific location of the Place and the Show Details or Edit Places links to see or change the details.
</p>
<% if flash[:notice] %>
    <div id="notice" style="color:blue"><br>
    <%= flash[:notice] %>  <% flash[:notice] = nil%> <br>
    </div>
   <% end %>  

 <div  style="float:left;"> <%= button_to 'Create New Master Place Name entry', new_master_place_name_path, method: :get %></div>
  <div style="float:left;">  <%= button_to_function "Previous Page", "history.back()" %> </div>
  <div style="float:left;"> <%= button_to" Logout", refinery.logout_path , method: :get%></div>
<div style="clear:both;"> </div><br>

<div id="main">
<table style="width: 100%">
  <tr>
    <th >Place</th>
    <th >Genuki page</th>
    <th >Source</th>
    <th >Last Updated</th>
    <th >Action</th>
  </tr>

<% @places.each do |place| %>
  <tr id="<%=place.id%>">
    <% unless place.disabled == "true" then %>
    <td >
     <%= place.place_name %>
    </td>
       
    <td >
    <% if (place.genuki_url.nil? || place.genuki_url == "no url") %>
      <%= place.genuki_url %>
     <% else %>
     <%= link_to('Genuki', place.genuki_url) %>
     <% end %>
    </td>
    <td >
     <%= place.source %>
    </td>
    <td >
     <%= place.u_at.strftime("%d %b %Y") %>
    </td>
    <td >
       <%= link_to 'Show Detail', master_place_name_path(place) %><br>     
       <%= link_to'Edit Place', edit_master_place_name_path(place) %><br>
       <%= link_to 'Discard', master_place_name_path(place),method: :delete,data:{confirm: 'Are you sure?'} %>
      </td>
      <% end %>
  </tr>
<% end %>
</table> <br />
</div>

 <div  style="float:left;"> <%= button_to 'Create New Master Place Name entry', new_master_place_name_path, method: :get %></div>
  <div style="float:left;">  <%= button_to_function "Previous Page", "history.back()" %> </div>
  <div style="float:left;"> <%= button_to" Logout", refinery.logout_path , method: :get%></div>
<div style="clear:both;"> </div><br>
 

 