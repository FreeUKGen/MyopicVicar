<% @freecen_individuals.each do |freecen_individual| %>
  <tr>
    <td><%= freecen_individual.sequence_in_household %></td>
    <td><%= freecen_individual.individual_flag %></td>
    <td><%= freecen_individual.surname %></td>
    <td><%= freecen_individual.forenames %></td>
    <td><%= freecen_individual.name_flag %></td>
    <td><%= freecen_individual.relationship %></td>
    <td><%= freecen_individual.marital_status %></td>
    <td><%= freecen_individual.sex %></td>
    <td><%= freecen_individual.age %></td>
    <td><%= freecen_individual.age_unit %></td>
    <td><%= freecen_individual.detail_flag %></td>
    <td><%= freecen_individual.occupation %></td>
    <td><%= freecen_individual.occupation_flag %></td>
    <td><%= freecen_individual.birth_county %></td>
    <td><%= freecen_individual.birth_place %></td>
    <td><%= freecen_individual.verbatim_birth_county %></td>
    <td><%= freecen_individual.verbatim_birth_place %></td>
    <td><%= freecen_individual.birth_place_flag %></td>
    <td><%= freecen_individual.disability %></td>
    <td><%= freecen_individual.language %></td>
    <td><%= freecen_individual.notes %></td>
    <td><%= freecen_individual.freecen1_vld_entry %></td>
    <td><%= link_to 'Show', freecen_individual %></td>
    <td><%= link_to 'Edit', edit_freecen_individual_path(freecen_individual) %></td>
    <td><%= link_to 'Destroy', freecen_individual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

