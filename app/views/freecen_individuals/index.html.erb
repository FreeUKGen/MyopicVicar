<table>
  <tr>
    <th>Sequence in household</th>
    <th>Individual flag</th>
    <th>Surname</th>
    <th>Forenames</th>
    <th>Name flag</th>
    <th>Relationship</th>
    <th>Marital status</th>
    <th>Sex</th>
    <th>Age</th>
    <th>Age unit</th>
    <th>Detail flag</th>
    <th>Occupation</th>
    <th>Occupation flag</th>
    <th>Birth county</th>
    <th>Birth place</th>
    <th>Verbatim birth county</th>
    <th>Verbatim birth place</th>
