<p style="text-align: center"><b>Listing of your files</b></p>
<div style="float:left;"> <%= button_to 'Upload CSV File', new_userid_detail_path, method: :get %> </div>

<div  style="float:left;"> <%= button_to 'Back to my Resource Management options',manage_resource_path(session[:user]), method: :get %> </div>
<div style="clear:both;"> </div><br>
<% if !session[:errors].nil? %>
  <% if flash[:notice] %>
    <div id="notice" style="color:red"><br>
    <%= flash[:notice] %> <br>
    </div>
   <% end %>
    
    <% else %>
    <% if flash[:notice] && session[:errors].nil? %>
    <div id="notice" style="color:green"><br>
    <%= flash[:notice] %> <br>
    </div>
   <% end %>
  <% end %>


<div id="main"style="color:black">
<% unless @freereg1_csv_files.nil? %>
<table CLASS="boldtable" >
  <tr>
    <th style="width:4%">Cnty</th>
    <th style="width:10%">Place</th>
    <th style="width:10%">Church</th>
    <th style="width:4%">Reg type</th>
    <th style="width:4%">Rec type</th>
    <th style="width:5%">Records</th>
   
    
    <th style="width:13%">UserID</th>
    <th style="width:10%">File name</th>
    <th style="width:9%">Loaded</th>
    <th style="width:4%">Errs</th>
    <th style="width:4%">Lock</th>
    <th style="width:13%">Syndicate</th>
    
    <th style="width:10%">Action</th>
  </tr>

<% @freereg1_csv_files.each do |freereg1_csv_file| %>
 <% if freereg1_csv_file.error == 0 %>
  <tr id="<%=freereg1_csv_file.id%>"  style="color:green">
 <% else %> 
<tr id="<%=freereg1_csv_file.id%>"  style="color:red">
 <% end %>
    <td><%= freereg1_csv_file.county %></td>
    <td><%= freereg1_csv_file.place %></td>
    <td><%= freereg1_csv_file.church_name %></td>
    <td><%= freereg1_csv_file.register_type %></td>
    <td><%= freereg1_csv_file.record_type %></td>
    <td><%= freereg1_csv_file.records %></td>
   
   
    <td><%= freereg1_csv_file.userid %></td>
    <td><%= freereg1_csv_file.file_name[0..-5] %></td>
    <td><%= freereg1_csv_file.uploaded_date.strftime("%d %b %Y") %></td>
   
    <td ><%= freereg1_csv_file.error  %></td>
    
    <td><%= freereg1_csv_file.locked %></td>
    <td><%= freereg1_csv_file.transcriber_syndicate %></td>
    
    <td><%= link_to 'Show', freereg1_csv_file %><br>
    <%= link_to 'Edit', edit_freereg1_csv_file_path(freereg1_csv_file) %><br>
    <% if freereg1_csv_file.error != 0 %>
     <%= link_to 'Errors', error_freereg1_csv_file_path(freereg1_csv_file)%><br>
     <% end %>
     <%= link_to 'Lock', lock_freereg1_csv_file_path(freereg1_csv_file), confirm: 'Are you sure you want to lock this file?'%><br>
    <%= link_to 'Destroy', freereg1_csv_file, confirm: 'Are you sure you want to delete this file?', method: :delete %></td>
  </tr>

<% end %>

</table>
<%else%>
   You have no files
   <%end%>
</div>

<br />
<div style="float:left;"> <%= button_to 'Upload CSV File', new_userid_detail_path, method: :get %> </div>

<div  style="float:left;"> <%= button_to 'Back to my Resource Management options', manage_resource_path(session[:user]), method: :get %> </div>
<div style="clear:both;"> </div><br>
