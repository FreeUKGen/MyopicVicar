Page Identification Freereg file upload:new<p>
<p style="text-align: center"><b>Upload a new Batch of records to FreeREG2</b></p>
  <% if @role =='counties' %>
<div  style="float:left;"> <%= button_to 'County Mangement Tools Page', manage_counties_path, method: :get %></div>
<%end%>
<% if @role =='syndicate' %>
<div  style="float:left;"> <%= button_to 'Syndicate Mangement Tools Page', manage_syndicates_path, method: :get %></div>
<%end%>

<div style="float:left;"> <%= button_to 'Resource Management options', manage_resource_path(@user), method: :get %> </div>

<div style="clear:both;"> </div><br>

 

  <% if flash[:notice] %>
    <div id="notice" style="color:blue"><br>
     <%= flash[:notice] %>  <% flash[:notice] = nil%> <br>
    </div>
   <% end %>  
     
    <div >
    <%= semantic_form_for @csvfile, html: { multipart: true } do |f| %>

  

     <%= f.input :csvfile, :label => "Please select file to upload  :" %><br>

      <%= f.input :userid, {:as => :select,  :collection => @people,:include_blank => false,:label => "Please select userid for whom file is to be loaded  :", :size => 5} unless session[:my_own] == 'my_own' %><br>
    
    
     <%= f.submit "Upload" %>
     <% end %>
    </div>
   
