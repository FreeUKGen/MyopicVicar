
<p style="text-align: center"><b>Upload a new Batch of records to FreeREG2</b></p>

  <% if flash[:notice] %>
    <div id="notice" style="color:blue"><br>
     <%= flash[:notice] %>  <% flash[:notice] = nil%> <br>
    </div>
   <% end %>  
     
    <div >
    <%= semantic_form_for @csvfile, html: { multipart: true } do |f| %>

  

     <%= f.input :csvfile, :label => "Please select file to upload  :" %><br>

      <%= f.input :userid, {:as => :select,  :collection => @people,:include_blank => false,:label => "Please select userid for whom file is to be loaded  :", :size => 5} unless session[:my_own] == 'my_own' %><br>
    
    
     <%= f.submit "Upload" %>
     <% end %>
    </div>
   
