citation for <%= application_name%>
<% case @type%>
<% when 'wikitree'%>
  <% if application_name == 'FreeREG'%>
    <%= render 'wikitree_freereg_citation', layout: false %>
  <% elsif application_name == 'FreeCEN'%>
    <%= render 'wikitree_freecen_citation', layout: false %>
  <% end%>
