
<section>
  <p>
      <%=@census_database %>, <%=@cen_year%> database, <%= application_name%> (<%=search_record_link(@search_record[:_id])%> : viewed <%= @viewed_date %>),
      <%=@searched_user_name %><% if !@is_family_head %> in household of <%= @family_head_name%><% else%> head of household<% end %>,
