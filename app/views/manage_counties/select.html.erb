<p style="text-align: center">Select the file<p>

 <%= semantic_form_for @manage_county do |f| %>
      <%= f.input :places, 
        { :as => :select, :label => 'Which batch do you want to review',
          :include_blank => false,
          :required => true,
          :collection => @files, :size => 10 } %>
    <%= f.action :submit, :as => :input, :label => 'Select File'   %>
   <% end %>



