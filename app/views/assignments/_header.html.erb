<% if params[:assignment_list_type] == 'group' || (!params[:assignment].nil? && params[:assignment][:assignment_list_type] == 'group') %>
  <div style="clear:both;"></div><br>
  <p style="text-align: center"><b>Images Assignment</b> of Image Group <b><%= @group.group_name %></b> of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>
<% else %>
  <div style="clear:both;"></div><br>
  <p style="text-align: center"><b>Assignments</b> for users in Syndicate <b><%= session[:syndicate] %></b>
<% end %>
