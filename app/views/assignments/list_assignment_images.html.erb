<% if session[:my_own] %>
  <% breadcrumb :my_own_assignment, @user %>
<% else %>
  <% breadcrumb :syndicate_all_assignment, @user, @county, @register, @source, @group %>
<% end %>

<% x = @assignment.first %>
<div style="clear:both;"></div><br>
<p style="text-align: center"><b><%= x[:fields][:groups][:group_name] %></b> assigned on <b><%= x[:fields][:assign_date].to_date.strftime("%Y-%m-%d") %> <%= ImageServerImage::Status::ALL_STATUSES[x[:fields][:images][:status]] %></b> for <b><%= x[:fields][:userids][:userid] %></b>
<%= render partial: 'flash_notice' %>

<% title "Assignment - list by userid" %>
<%= render partial: 'flash_notice' %>

<%= render 'assignment_list' %>
