<% breadcrumb :freecen2_pieces_district,  @freecen2_district.id, @chapman_code  %>
<h2 style="text-align: center">FreeCen2 Pieces for <%=@freecen2_district%> in <%=@chapman_code%></h2>
<%= render 'flash_notice' %>
<<table style="margin-left:25%" class="table--bordered t50 table--data table--striped " >
  <thead >
    <tr >
      <th >Year</th>
      <th >Pieces</th>
    </tr>
  </thead>
  <% Freecen::CENSUS_YEARS_ARRAY.each.each do |year| %>
    <tr >
      <td> <%=  link_to "#{year}", pieces_district_year_index_path(@freecen2_district.id, chapman_code:"#{@chapman_code}",year:"#{year}"), method: :get, class: 'btn   btn--small'%> </td>
      <td> <%= @totals_pieces[year] %></td>
    </tr>
  <% end%>
  <tr >
    <td> Total</td>
    <td> <%= @grand_totals_pieces%></td>
  </tr>
</table>