<% breadcrumb :spreadsheet_selection,  @freecen_csv_file%>
<h2 class=" text--center">Select the spreadsheet file to be downloaded</h2>
<div class="island  island--light">
  <%= render partial: 'flash_notice' %>
  <%= semantic_form_for @freecen_csv_file, :html => { :class => "grid"}  do |f| %>
    <%= f.input :action,
    { :as => :select, :include_blank => true,  :label => "Make a Selection", :required => false, :collection => @options,:width => 10 ,:input_html => {:class => " simple_form_bgcolour simple_form_position overide_selection_field_width", :size => 10 ,'onchange' => @location} }%>
  <% end %>
</div>