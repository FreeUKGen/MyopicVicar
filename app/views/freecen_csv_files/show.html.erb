<% breadcrumb :show_file,  @freecen_csv_file %>
<%= render 'show_header' %>
<div class="grid">
  <section class="island ">
    <div class="grid__item ">
      <table   class="table--bordered  table--data">
        <colgroup >
          <col class=t40>
          <col class=t60>
        </colgroup>
        <tr>
          <th class="caps">Field</th>
          <th  class="caps">Value</th>
        </tr>
        <tr>
          <td>County </td>
          <td class="weight--semibold"> <%=  county_name(@freecen_csv_file)%>  </td>
        </tr>
        <tr>
          <td> Place name </td>
          <td class="weight--semibold"> <%= place_name(@freecen_csv_file) %>  </td>
        </tr>
        <tr>
          <td>Record type </td>
          <td class="weight--semibold">  <%= RecordType::display_name(@freecen_csv_file.record_type) %> </td>
        </tr>
        <tr>
          <td>Number of records </td>
          <td class="weight--semibold"> <%= @freecen_csv_file.records %> </td>
        </tr>
        <tr>
          <td>Earliest year in batch </td>
          <td class="weight--semibold"><%= @freecen_csv_file.datemin %> </td>
        </tr>
        <tr>
          <td>Latest year in batch </td>
          <td class="weight--semibold"> <%= @freecen_csv_file.datemax %></td>
        </tr>
        <tr>
          <td>Uploaded by userid </td>
          <td class="weight--semibold"> <%= @freecen_csv_file.userid %> </td>
        </tr>
        <tr>
          <td>Batch name</td>
          <td class="weight--semibold"><%= @freecen_csv_file.file_name %></td>
        </tr>
        <tr>
          <td>Transcribed by </td>
          <td class="weight--semibold">  <%= @freecen_csv_file.transcriber_name %></td>
        </tr>
        <tr>
          <td>Transcribers email address </td>
          <td class="weight--semibold"> <%= @freecen_csv_file.transcriber_email %></td>
        </tr>
        <tr>
          <td>Syndicate transcribed for </td>
          <td class="weight--semibold">  <%= @freecen_csv_file.transcriber_syndicate %></td>
        </tr>
        <tr>
          <td>Date of upload </td>
          <td class="weight--semibold"> <%= @freecen_csv_file.uploaded_date.strftime("%d %b %Y") unless @freecen_csv_file.uploaded_date.nil? %></td>
        </tr>
        <% case%>
        <% when @freecen_csv_file.error == 0%>
        <tr id="<%= @freecen_csv_file.id%>"  style="color:black" >
          <% when  @freecen_csv_file.error > 0 %>
          <tr id="<%= @freecen_csv_file.id%>"  style="color:red" >
          <% end %>
          <td>Number of errors</td>
          <td class="weight--semibold"> <%= file_errors(@freecen_csv_file)%></td>
        </tr>
        <% if @freecen_csv_file.locked_by_transcriber ||  !@freecen_csv_file.locked_by_transcriber == "false"%>
          <tr id="<%= @freecen_csv_file.id%>"  style="color:darkviolet" >
          <% else%>
            <tr id="<%= @freecen_csv_file.id%>"  style="color:black" >
            <% end %>
            <td>Batch is locked from overriding by Transcriber</td>
            <td class="weight--semibold"> <%= transform_boolean(@freecen_csv_file.locked_by_transcriber) %></td>
          </tr>
          <% if !@freecen_csv_file.locked_by_coordinator == "false" || @freecen_csv_file.locked_by_coordinator  %>
            <tr id="<%= @freecen_csv_file.id%>"  style="color:darkviolet" >
            <% else%>
              <tr id="<%= @freecen_csv_file.id%>"  style="color:black" >
              <% end %>
              <td>Batch is locked from overriding by Coordinator</td>
              <td class="weight--semibold"> <%= transform_boolean(@freecen_csv_file.locked_by_coordinator) %></td>
            </tr>
            <tr>
              <td>Credit the following for the transcription</td>
              <td class="weight--semibold"><%= @freecen_csv_file.credit_name %></td>
            </tr>
            <tr>
              <td>Email address for the person who has been credited </td>
              <td class="weight--semibold"><%= @freecen_csv_file.credit_email %></td>
            </tr>
            <tr>
              <td>Date transcribed </td>
              <td class="weight--semibold">  <%= @freecen_csv_file.transcription_date %></td>
            </tr>
            <tr>
              <td>Date modified</td>
              <td class="weight--semibold"><%= @freecen_csv_file.modification_date %></td>
            </tr>
            <tr>
              <td>Image numbers available with record</td>
              <td class="weight--semibold">  <%= @freecen_csv_file.lds %> </td>
            </tr>
            <tr>
              <td>Character set of original file</td>
              <td class="weight--semibold"><%= @freecen_csv_file.characterset %></td>
            </tr>
            <tr>
              <td>Processed (No means that it was only verified for errors and its contents are NOT in the searchable database)</td>
              <td class="weight--semibold"><%= transform_boolean(@freecen_csv_file.processed) %></td>
            </tr>
            <tr>
              <td>Processed Date</td>
              <td class="weight--semibold"><%= @freecen_csv_file.processed_date.strftime("%d %b %Y") unless @freecen_csv_file.processed_date.nil? %></td>
            </tr>
            <tr>
              <td>Flexible CSV Used</td>
              <td class="weight--semibold"><%= transform_boolean(@freecen_csv_file.def) %></td>
            </tr>
            <tr>
              <td>Column Order</td>
              <td class="weight--semibold"><%= @freecen_csv_file.order %></td>
            </tr>
          </table>
        </div>
      </section>
    </div>
    <br>