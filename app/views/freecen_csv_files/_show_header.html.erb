<h3 style="text-align: center"><b><%= file_name(@freecen_csv_file) %> (<%= userid(@freecen_csv_file) %>) in <b><%= @piece.district_name %></b> of <b><%= @piece.chapman_code %></b><br>
  </h3>
  <div  style="text-align: center">
    <%= replace_freecen_file %>
    <%= download_freecen_file %>
    <%= remove_freecen_file %>
    <%= download_messages %><br>
    <%= list_freecen_file_flags %>
    <%= list_freecen_file_error_entries %>
    <%= list_freecen_file_warning_entries %>
    <%= list_freecen_file_information_entries %>
    <%= browse_freecen_file_civil_parishes%>
    <%= browse_freecen_file_pages%>
    <%= browse_freecen_file_dwellings%>
    <%= browse_freecen_file_individuals%>
    <br>
    <% if ['county_coordinator', 'country_coordinator', 'system_administrator', 'data_manager', 'validator'].include?(@user.person_role) %>
      <%= change_freecen_file_owner  %>
      <%= edit_freecen_file %>
      <%= reprocess_freecen_file %>
      <%= validate_freecen_file %>
    <% end%>
    <% if ['system_administrator','data_manager'].include?(@user.person_role) %>
      <%= delete_freecen_file %>
    <% end%>
  </div>
  <div style="clear:both;"> </div>
  <br>
  <%= render partial: 'flash_notice' %>