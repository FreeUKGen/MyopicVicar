<%= simple_form_for @freecen_csv_file , :html => { :class => "grid"}  do |f| %>
  <%= f.input :transcriber_name , :input_html => {:class => " simple_form_bgcolour ",  :size => 30 }%>
  <%= f.input :transcriber_email, :input_html => {:class => " simple_form_bgcolour ", :size => 30 } %>
  <%= f.input :transcription_date ,:input_html => {:class => "simple_form_bgcolour ", :size => 30  }%>
  <%= f.input :modification_date ,:input_html => {:class => " simple_form_bgcolour ", :size => 30}%>
  <% if session[:my_own] || session[:role] == "county_coordinator" || session[:role] == 'syndicate_coordinator' || session[:role] == 'system_administrator'%>
    <%= f.input :locked_by_transcriber, :label => "Locked by transcriber ",:include_blank => false,
      :collection => [ true, false],:input_html => {:class => " simple_form_bgcolour simple_form_position overide_selection_field_width",  :size => 2} %>
    <% if session[:my_own]%>
      <%= f.input :locked_by_coordinator, :input_html => {:class => "   overide_selection_field_width",:disabled => true, :size => 30 } %>
    <%else%>
      <%= f.input :locked_by_coordinator, :label => "Locked by coordinator ",:include_blank => false,
          :collection => [ true, false],:input_html => {:class => " simple_form_bgcolour simple_form_position overide_selection_field_width",  :size => 2} %>
    <% end %>
  <%else%>
    <%= f.input :locked_by_transcriber, :input_html => { :disabled => true, :size => 30 } %>
    <%= f.input :locked_by_coordinator, :input_html => {:disabled => true, :size => 30 } %>
  <% end %>
  <%= f.button :submit, 'Submit'   %>
<% end %>