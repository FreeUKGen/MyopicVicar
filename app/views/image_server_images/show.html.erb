<% if session[:my_own] %>
	<% case session[:my_own_list] %>
		<% when 'syndicate' %>
  		<% breadcrumb :my_list_by_syndicate_image_server_image, @user, @county, @place, @church, @register, @source, @group %>
		<% when 'county' %>
  		<% breadcrumb :my_list_by_county_image_server_image, @user, @county, @place, @church, @register, @source, @group %>
  <% end %>
<% elsif session[:manage_user_origin] == 'manage syndicate' %>
	<% breadcrumb :syndicate_image_server_image, @county, @place, @church, @register, @source, @group %>
<% else %>
	<% case session[:ig_allocation] %>
		<% when 'by_syndicate' %>
			<% breadcrumb :sort_by_syndicate_image_server_image, @county, @place, @church, @register, @source, @group %>
		<% when 'by_place' %>
			<% breadcrumb :sort_by_place_image_server_image, @county, @place, @church, @register, @source, @group %>
		<% else %>
			<% breadcrumb :county_image_server_image, @county, @place, @church, @register, @source, @group %>
	<% end %>
<% end %>

<% title "IS Image - properties" %>
<div style="clear:both;"></div><br>
<p style="text-align: center"><b>Images</b> of Image Group <b><%= @group.group_name %></b> of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>

<% if flash[:notice] %>
  <div id="notice" style="color:blue">
   	<%= flash[:notice] %><% flash.clear %>
  </div><br>
<% end %>

<div class="grid">
  <section class="island">
    <%= render 'detail' %>
  </section>
</div>
