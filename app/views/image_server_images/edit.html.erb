<% if session[:manage_user_origin] == 'manage syndicate' %>
	<% breadcrumb :syndicate_show_image, @county, @place, @church, @register, @source, @group %>
<% else %>
	<% breadcrumb :show_image, @county, @place, @church, @register, @source, @group %>
<% end %>

<div style="clear:both;"></div><br>
<p style="text-align: center">Images of Image Group <b><%= @group.group_name %></b> of the <b><%= @register_type %></b> register for <b><%= @church_name %></b> in <b><%= @place_name %></b> of <b><%= @county %></b>

<% if flash[:notice] %>
  <div id="notice" style="color:blue">
   	<%= flash[:notice] %><% flash.clear %>
  </div><br>
<% end %>

<%= render 'edit' %>

