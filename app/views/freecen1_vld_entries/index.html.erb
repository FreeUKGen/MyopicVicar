<%= render 'index_header' %>
<div class="scrollable ">
  <table class="table--bordered table--data" >
    <tr>
      <th class='sticky-header'>Dwelling number</th>
      <th class='sticky-header'>Sequence in household</th>
      <th class='sticky-header'>Civil parish</th>
      <th class='sticky-header'>Ecclesiastical parish</th>
      <th class='sticky-header'>Enumeration district</th>
      <th class='sticky-header'>Folio number</th>
      <th class='sticky-header'>Page number</th>
      <th class='sticky-header'>Schedule number</th>
      <th class='sticky-header'>House number</th>
      <th class='sticky-header'>House or street name</th>
      <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
        <th class='sticky-header'>Uninhabited flag</th>
        <th class='sticky-header'>Unoccupied notes</th>
        <th class='sticky-header'>Individual flag</th>
      <% end %>
      <th class='sticky-header'>Surname</th>
      <th class='sticky-header'>Forenames</th>
      <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
        <th class='sticky-header'>Name flag</th>
      <% end %>
      <th class='sticky-header'>Relationship</th>
      <th class='sticky-header'>Marital status</th>
      <th class='sticky-header'>Sex</th>
      <th class='sticky-header'>Age</th>
      <th class='sticky-header'>Age unit</th>
      <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
        <th class='sticky-header'>Detail flag</th>
      <% end %>
      <th class='sticky-header'>Occupation</th>
      <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
        <th class='sticky-header'>Occupation flag</th>
      <% end %>
      <th class='sticky-header'>Verbatim birth county</th>
      <th class='sticky-header'>Verbatim birth place</th>
      <% if session[:vld_pob_val].present? && session[:vld_pob_val]%>
        <th class='sticky-header'>Alternative Birth county</th>
        <th class='sticky-header'>Alternative Birth place</th>
      <% else %>
        <th class='sticky-header'>Birth county</th>
        <th class='sticky-header'>Birth place</th>
      <% end %>
      <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
        <th class='sticky-header'>Birth place flag</th>
      <% end %>
      <th class='sticky-header'>Disability</th>
      <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
        <th class='sticky-header'>Language</th>
      <% end %>
      <th class='sticky-header'>Notes</th>
      <% if session[:vld_pob_val].present? && session[:vld_pob_val]%>
        <th class='sticky-header'>POB Valid</th>
        <th class='sticky-header'>POB Warning</th>
      <% else %>
        <th class='sticky-header'>Deleted flag</th>
      <% end %>
      <th colspan = "2" class='sticky-header '>Actions</th>
    </tr>
    <% @freecen1_vld_entries.each do |freecen1_vld_entry| %>
      <tr id="<%= freecen1_vld_entry.id %>">
        <td><%= freecen1_vld_entry.dwelling_number %></td>
        <td><%= freecen1_vld_entry.sequence_in_household %></td>
        <td><%= freecen1_vld_entry.civil_parish %></td>
        <td><%= freecen1_vld_entry.ecclesiastical_parish %></td>
        <td><%= freecen1_vld_entry.enumeration_district %></td>
        <td><%= freecen1_vld_entry.folio_number %></td>
        <td><%= freecen1_vld_entry.page_number %></td>
        <td><%= freecen1_vld_entry.schedule_number %></td>
        <td><%= freecen1_vld_entry.house_number %></td>
        <td><%= freecen1_vld_entry.house_or_street_name %></td>
        <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
          <td><%= freecen1_vld_entry.uninhabited_flag %></td>
          <td><%= freecen1_vld_entry.unoccupied_notes %></td>
          <td><%= freecen1_vld_entry.individual_flag %></td>
        <% end %>
        <td><%= freecen1_vld_entry.surname %></td>
        <td><%= freecen1_vld_entry.forenames %></td>
        <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
          <td><%= freecen1_vld_entry.name_flag %></td>
        <% end %>
        <td><%= freecen1_vld_entry.relationship %></td>
        <td><%= freecen1_vld_entry.marital_status %></td>
        <td><%= freecen1_vld_entry.sex %></td>
        <td><%= freecen1_vld_entry.age %></td>
        <td><%= freecen1_vld_entry.age_unit %></td>
        <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
          <td><%= freecen1_vld_entry.detail_flag %></td>
        <% end %>
        <td><%= freecen1_vld_entry.occupation %></td>
        <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
          <td><%= freecen1_vld_entry.occupation_flag %></td>
        <% end %>
        <td><%= freecen1_vld_entry.verbatim_birth_county %></td>
        <td><%= freecen1_vld_entry.verbatim_birth_place %></td>
        <td><%= freecen1_vld_entry.birth_county %></td>
        <td><%= freecen1_vld_entry.birth_place %></td>
        <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
          <td><%= freecen1_vld_entry.birth_place_flag %></td>
        <% end %>
        <td><%= freecen1_vld_entry.disability %></td>
        <% unless session[:vld_pob_val].present? && session[:vld_pob_val]%>
          <td><%= freecen1_vld_entry.language %></td>
        <% end %>
        <td><%= freecen1_vld_entry.notes %></td>
        <% if session[:vld_pob_val].present? && session[:vld_pob_val]%>
          <td><%= freecen1_vld_entry.pob_valid %></td>
          <td><%= freecen1_vld_entry.pob_warning %></td>
          <td><%= link_to 'Edit', edit_pob_freecen1_vld_entry_path(id: freecen1_vld_entry.id) %></td>
          <td><%= link_to 'Override POB Status', override_pob_status_freecen1_vld_entry_path(id: freecen1_vld_entry.id), method: :get, data: { confirm:  'Are you sure you want to Override the POB Status for this entry?'} %></td>
        <% else %>
          <td><%= freecen1_vld_entry.deleted_flag %></td>
          <td><%= link_to 'Show', freecen1_vld_entry_path(id: freecen1_vld_entry.id,file: @freecen1_vld_file.id) %></td>
        <% end %>
      </tr>
    <% end %>
  </table>
</div>
<br>
<br>