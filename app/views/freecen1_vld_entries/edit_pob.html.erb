<h3 style="text-align: center">Editing  Dwelling Number: <%= @freecen1_vld_entry.dwelling_number %> - Sequence in Household: <%= @freecen1_vld_entry.sequence_in_household %> ( <%= @freecen1_vld_entry.forenames %> - <%= @freecen1_vld_entry.surname %> ) </h3>
<%= render 'flash_notice' %>
<%= semantic_form_for @freecen1_vld_entry, :html => { :class => 'grid'}   do |f| %>
  <% if %w[data_manager system_administrator].include?(@user.person_role)%>
    <%= f.input :pob_valid , :label => 'POB Validation Status', :input_html => {:class => ' simple_form_bgcolour  ',  :size => 10, :disabled => true}%>
    <%= f.input :pob_warning , :label => 'POB Warning Message', :input_html => {:class => ' simple_form_bgcolour  ',  :size => 45, :disabled => true}%>
    <%= f.input :verbatim_birth_county ,  label: 'Verbatim Birth County; current value highlighted',selected: @freecen1_vld_entry.verbatim_birth_county, collection: @counties, :input_html =>{:class => ' simple_form_bgcolour small_selection_field_width', :size => 4} %>
    <%= f.input :verbatim_birth_place ,  :input_html => {:class => ' simple_form_bgcolour  ',  :size => 30 }%>
    <%= f.input :birth_county ,  label: 'Birth County; current value highlighted',selected: @freecen1_vld_entry.birth_county, collection: @counties, :input_html =>{:class => ' simple_form_bgcolour small_selection_field_width', :size => 4} %>
    <%= f.input :birth_place ,  :input_html => {:class => ' simple_form_bgcolour  ',  :size => 30 } %>
    <%= f.input :notes, :as => :text, :input_html => {:class => 'simple_form control-label simple_form_bgcolour ',   :placeholder => 'Notes about entry in census',  :size => '50x5' } %>
    <div  style='text-align: center'>
      <%= f.submit 'Submit',  class: "btn btn--gamma", title: 'Saves changes.'%>
      <%= link_to 'Cancel', manual_validate_pobs_freecen1_vld_file_path(id: @freecen1_vld_entry.freecen1_vld_file_id), method: :get, class: "btn btn--gamma", title: 'Cancels the Editing session.'%>
    </div>
    <div style='text-align: center'>
      <%= link_to 'Override POB Status', override_pob_status_freecen1_vld_entry_path(id: @freecen1_vld_entry.id), method: :get, class: "btn btn--gamma", data: { confirm:  'Are you sure you want to Override the POB Status for this entry?'} %>
    <% end %>
  </div>
<% end %>