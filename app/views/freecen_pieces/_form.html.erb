<%= simple_form_for(@freecen_piece) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :chapman_code %>
    <%= f.input :piece_number %>
    <%= f.input :district_name %>
    <%= f.input :subplaces %>
    <%= f.input :parish_number %>
    <%= f.input :suffix %>
    <%= f.association :freecen1_fixed_dat_entry %>
    <%= f.association :place %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
